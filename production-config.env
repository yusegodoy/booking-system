# CONFIGURACIÓN PARA PRODUCCIÓN
# IMPORTANTE: Cambiar estos valores según tu configuración de Ionos

# URL del backend en producción
REACT_APP_API_BASE_URL=https://airportshuttletpa-backend.railway.app/api

# Google Maps API Key (la misma que tienes)
REACT_APP_GOOGLE_MAPS_API_KEY=YOUR_GOOGLE_MAPS_API_KEY_HERE

# Configuración de base de datos MongoDB
MONGODB_URI=mongodb://localhost:27017/booking-system

# Puerto del servidor backend
PORT=5001

# Configuración de email (para notificaciones)
EMAIL_HOST=smtp.ionos.com
EMAIL_PORT=587
EMAIL_USER=tu-email@tu-dominio.com
EMAIL_PASS=YOUR_EMAIL_PASSWORD_HERE

# Configuración de Google Calendar
GOOGLE_CLIENT_ID=YOUR_GOOGLE_CLIENT_ID_HERE
GOOGLE_CLIENT_SECRET=YOUR_GOOGLE_CLIENT_SECRET_HERE
GOOGLE_REDIRECT_URI=https://airportshuttletpa.com/auth/google/callback

# JWT Secret (generar uno nuevo para producción)
JWT_SECRET=YOUR_SUPER_SECURE_JWT_SECRET_HERE

# Configuración de CORS
CORS_ORIGIN=https://airportshuttletpa.com
